long long kemungkinan(int n) {
    long long hasil = 1;
    for (int i = 1; i <= n; i++) {
        hasil *= i;
    }
    return hasil;
}

bool mirip(string x, string y) {
    int fx[10] = {0};
    int fy[10] = {0};

    for (int i = 0; i < x.length(); i++) {
        fx[x[i] - '0']++;
    }
    for (int i = 0; i < y.length(); i++) {
        fy[y[i] - '0']++;
    }

    for (int i = 0; i < 10; i++) {
        if (fx[i] != fy[i]) return false;
    }
    return true;
}

long findSimilar(string a, string b) {

    string target;
    if (mirip(a, b)) {
        target = a;
    } else {
        target = b;
    }

    int freq[10] = {0};
    int n = target.length();

    for (int i = 0; i < n; i++) {
        freq[target[i] - '0']++;
    }

    long long total = kemungkinan(n);
    for (int i = 0; i < 10; i++) {
        total /= kemungkinan(freq[i]);
    }

    if (freq[0] > 0) {
        freq[0]--;
        long long invalid = kemungkinan(n - 1);
        for (int i = 0; i < 10; i++) {
            invalid /= kemungkinan(freq[i]);
        }
        total -= invalid;
    }

    return total;
}